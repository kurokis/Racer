    <link name="camera_link">
      <!--forward:-y, upward:+z -->
      <pose>0 -0.2 0.15 0 0 -1.57</pose>
      <sensor type="camera" name="camera">
        <!-- Set always_on only sensor, not on plugin -->
        <always_on>true</always_on>
        
        <!-- Set update_rate only sensor, not on plugin -->
        <update_rate>30</update_rate>
        <camera name="camera_name">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>100</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <!-- Noise is sampled independently per pixel on each frame.
               That pixel's noise value is added to each of its color
               channels, which at that point lie in the range [0,1]. -->
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
          <distortion>
            <k1>0.</k1>
            <k2>0.</k2>
            <k3>0.</k3>
            <p1>0.</p1>
            <p2>0.</p2>
            <center>0.5 0.5</center>
          </distortion>
        </camera>
        
        <!-- Use camera, not camera_triggered -->
        <plugin name="camera_plugin" filename="libgazebo_ros_camera.so">
          <!-- Change namespace, camera name and topics so:
               * Images are published to: /cam/camera/image_raw
               * Camera info is published to: /cam/camera/camera_info 
          -->
          <ros>
            <namespace>cam</namespace>
            <!--
            <argument>image_raw:=custom_img</argument>
            <argument>camera_info:=custom_info</argument>
            -->
          </ros>
          <!-- Set camera name. If empty, defaults to sensor name (i.e. "sensor_name") -->
          <camera_name>camera</camera_name>
          <!-- Set TF frame name. If empty, defaults to link name (i.e. "link_name") -->
          <frame_name>custom_frame</frame_name>
          <hack_baseline>0.07</hack_baseline>
          <!-- No need to repeat distortion parameters or to set autoDistortion -->
        </plugin>
      </sensor>
    </link>
    
